    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "type-check": "tsc --noEmit",
    "android:install": "npm install @capacitor/core @capacitor/cli @capacitor/android @capacitor/geolocation @capacitor/network @capacitor/splash-screen @capacitor/status-bar @capacitor/push-notifications @capacitor/local-notifications @capacitor/app @capacitor/preferences",
